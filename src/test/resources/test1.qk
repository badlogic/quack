struct Array <T> {
	val len: i32	
	val ptr: i64
	
	def Array(len: i32=0) {
		self.len = len
		self.ptr = __intrinsic_alloc<T>(len * sizeof(T))
	}
	
	def []get(index: i32): T {
		return __intrinsic_get<T>(ptr + index * sizeof(T))
	}
	
	def []set(index: i32, value: T) {
		__intrinsic_set<T>(ptr + index * sizeof(T), value)
	}
}

